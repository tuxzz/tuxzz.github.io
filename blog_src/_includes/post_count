{% assign post_count = "0" %}
{% for post in site.posts %}
    {% assign notsos = true %}
    {% for category in post.categories %}
        {% if category == "sky of sora" %}
            {% assign notsos = false %}
        {% endif %}
    {% endfor %}
    {% if notsos %}
        {% assign post_count = post_count | plus:1 %}
    {% endif %}
{% endfor %}
